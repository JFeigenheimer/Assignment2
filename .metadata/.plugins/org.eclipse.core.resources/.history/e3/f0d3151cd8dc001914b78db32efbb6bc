import java.util.Scanner;

public class poll 
{



   public static void main(String[] args)
   {
      Scanner scnr = new Scanner(System.in);
      boolean flag = false;
      String check;
      String[] topics = new String[5];
      int[][] responses = new int[5][10];
      int response;
      topics[0] = "Politics";
      topics[1] = "Climate";
      topics[2] = "Videogames";
      topics[3] = "Sports";
      topics[4] = "Food";

      System.out.println("Please rate these topics from 1-10 on how important they are to you.");
      
      while (flag != true)
      {
        for(int i = 0; i < 5; i++)
        {
          System.out.printf("%s: \n", topics[i]);
          response = scnr.nextInt();
          responses[i][response - 1]++;
        }

        System.out.println("Do you wish to continue? y/n");
        check =scnr.nextLine();
        check = check.replaceAll("\\s+", "");
        System.out.println(check.toLowerCase());
        System.out.println(flag);
        if (check.toLowerCase() == "n")
        	flag = true;
        System.out.println(check.toLowerCase());
        System.out.println(flag);
      }
      
      scnr.close();
   }
	
}